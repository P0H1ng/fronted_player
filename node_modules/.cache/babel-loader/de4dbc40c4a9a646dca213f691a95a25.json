AAA,EAAA;AAAA;AApCA,CAAA","sourcesContent":["<template>\n    <div>\n        <v-dialog\n                v-model=\"showGameBoxesVisible\"\n                width=\"700\"\n        >\n            <v-card>\n                <v-card-title class=\"headline\">\n                    {{$t('gamebox.list')}}\n                </v-card-title>\n\n                <v-card-text>\n                    <v-simple-table :dense=\"true\">\n                        <template v-slot:default>\n                            <thead>\n                            <tr>\n                                <th class=\"text-left\">{{$t('gamebox.team')}}</th>\n                                <th class=\"text-left\">{{$t('gamebox.challenge')}}</th>\n                                <th class=\"text-left\">{{$t('gamebox.address')}}</th>\n                            </tr>\n                            </thead>\n                            <tbody>\n                            <tr v-for=\"(item, index) in allGameBoxes\" v-bind:key=\"index\">\n                                <td>{{ item.TeamName }}</td>\n                                <td>{{ item.ChallengeName }}</td>\n                                <td>{{ item.IP }}:{{ item.Port }}</td>\n                            </tr>\n                            </tbody>\n                        </template>\n                    </v-simple-table>\n                </v-card-text>\n\n                <v-divider></v-divider>\n\n                <v-card-actions>\n                    <v-spacer></v-spacer>\n                    <v-btn color=\"primary\" text @click=\"showGameBoxesVisible = false\">\n                        {{$t('general.close')}}\n                    </v-btn>\n                </v-card-actions>\n            </v-card>\n        </v-dialog>\n\n        <v-card v-if=\"info !== null\">\n            <v-list-item two-line>\n                <v-list-item-content>\n                    <div>\n                        <v-img v-if=\"info.Logo !== ''\" class=\"logo\"\n                               :src=\"`${utils.baseURL}/uploads/${info.Logo}`\"></v-img>\n                        <v-list-item-title class=\"headline\">{{info.Name}}</v-list-item-title>\n                        <v-list-item-subtitle>\n                            Token {{ info.Token }}\n                        </v-list-item-subtitle>\n                    </div>\n                </v-list-item-content>\n            </v-list-item>\n\n            <v-card-text>\n                <GameBox/>\n            </v-card-text>\n            <v-divider/>\n            <v-card-actions>\n                <v-list-item>\n                    #{{ info.Rank }} / {{ utils.FormatFloat(info.Score) }} {{$t('general.score')}}\n                </v-list-item>\n                <v-btn v-if=\"allGameBoxes !== null\" text @click=\"showGameBoxesVisible = true\">靶機列表</v-btn>\n            </v-card-actions>\n        </v-card>\n    </div>\n</template>\n\n<script>\n    import GameBox from \"./GameBox\";\n\n    export default {\n        name: \"Info\",\n        data() {\n            return {\n                info: null,\n                timer: null,\n                allGameBoxes: null,\n\n                showGameBoxesVisible: false,\n            }\n        },\n        mounted() {\n            this.timer = setInterval(this.getInfo, 5000)\n            this.getInfo()\n            this.getAllGameBoxes()\n        },\n\n        beforeDestroy() {\n            clearInterval(this.timer)\n        },\n\n        methods: {\n            getInfo() {\n                this.utils.GET(\"/team/info\").then(res => {\n                    this.info = res\n                })\n            },\n\n            getAllGameBoxes() {\n                this.utils.GET(\"/team/gameboxes/all\").then(res => {\n                    this.allGameBoxes = res\n                }).catch(() => {\n                    this.allGameBoxes = null\n                })\n            }\n        },\n        components: {GameBox}\n    }\n</script>\n\n<style scoped>\n    .logo {\n        float: left;\n        width: 50px;\n        height: 50px;\n        margin-right: 5px;\n    }\n</style>"],"sourceRoot":"src/components"}]}